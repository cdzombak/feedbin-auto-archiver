// Ages in this file are specified in days.
{
  // The --max-age argument will be ignored when using a rules file that specifies max_age globally.
  "max_age": 45,

  // feed_specific is also required.
  // It could be an empty list (`[]`), but then why are you using a rules file?
  "feed_specific": [
    {
      // Add comment with Feed Name for maintainability
      "feed_id": 450,
      "max_age": 1
    },
    {
      // Another Feed
      "feed_id": 10761,
      "keep_n": 2
    },
    {
      // Feed with both max_age and keep_n (archives if EITHER condition is met)
      "feed_id": 12345,
      "max_age": 14,
      "keep_n": 5
    },
  ]
}
